<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="CommonCodeMapper">

  <select id="findCommonCode" parameterType="HashMap" resultType="commonCode">
    SELECT *
    FROM common_code
    WHERE type = #{type}
    <if test="name != null">
      AND name = #{name}
    </if>
  </select>

</mapper>